<ReadOtpSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- For boolean values (true/false) you have to use lower case letter -->
  <DisableModulePresentCheck>false</DisableModulePresentCheck>

  <!-- Verify that data in OTP is that same as in the WriteOtpModuleSettings -->
