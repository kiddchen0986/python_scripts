<?xml version="1.0" encoding="utf-8"?>

<ReadOtpSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <!-- For boolean values (true/false) you have to use lower case letter -->
    <DisableModulePresentCheck>true</DisableModulePresentCheck>

    <!-- Verify that data in OTP is that same as in the WriteOtpModuleSettings -->
    <VerifyAgainstWriteOtpModuleSettings>false</VerifyAgainstWriteOtpModuleSettings>

    <!-- Verify that data in OTP is that same as in the WriteOtpVendorSettings-->
    <VerifyAgainstWriteOtpVendorSettings>false</VerifyAgainstWriteOtpVendorSettings>

    <!-- Accept bit errors in OTP memory without failing -->
    <AcceptBitErrors>true</AcceptBitErrors>

    <!-- Enable check of OTP package section -->
    <EnablePackageSectionCheck>true</EnablePackageSectionCheck>
</ReadOtpSettings>