<?xml version="1.0" encoding="utf-8" ?>
<ModuleQualitySettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!--
        Specifies how much to crop the image for calibration (does not affect analysis).
    -->
  <CalibrationCrop>0,0,0,0</CalibrationCrop>
  <CaptureSettings>
    <AdcShift>6</AdcShift>
    <AdcGain>7</AdcGain>
    <PixelGain>0</PixelGain>
    <DacOffset>20</DacOffset>
  </CaptureSettings>
  <!-- Timeout for finger detect in ms-->
  <TimeOutWaitForFingerDetect>10000</TimeOutWaitForFingerDetect>
  <!-- Time to stabilize finger after finger detect in ms -->
  <StabilizeAfterForFingerDetect>500</StabilizeAfterForFingerDetect>

  <!--
        Specifies how much to crop the image for analysis.
    -->
  <AnalysisCrop>0,0,0,0</AnalysisCrop>
  <SnrLimit>3</SnrLimit>
  <!-- 0 is default value, other values are used in specific cases -->
  <LimitsPreset>0</LimitsPreset>
</ModuleQualitySettings>
