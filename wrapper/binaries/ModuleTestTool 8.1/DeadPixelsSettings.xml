<?xml version="1.0" encoding="utf-8"?>
<DeadPixelsSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <!-- For boolean values (true/false) you have to use lower case letter -->
    <!-- The setting OverrideDefaultCriteria details if the criteria value(s) below is/are to be used or not. If false, the default value(s) is/are used. -->
    <OverrideDefaultCriteria>false</OverrideDefaultCriteria>
    <!-- The upper limit of the number of dead pixels that are allowed -->
    <PixelErrorsUpperLimit>10</PixelErrorsUpperLimit>
    <MaxDeviation>25</MaxDeviation>
</DeadPixelsSettings>